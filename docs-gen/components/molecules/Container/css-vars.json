{
  "--mbx-container-background": {
    "description": "Fallback value for `--mbx-container-background-light` and `--mbx-container-background-dark`"
  },
  "--mbx-container-background-light": {
    "description": "Container background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-container-background"
  },
  "--mbx-container-background-dark": {
    "description": "Container background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-container-background"
  },
  "--mbx-container-text": {
    "description": "Fallback value for `--mbx-container-text-light` and `--mbx-container-text-dark`"
  },
  "--mbx-container-text-light": {
    "description": "Container text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-text-light",
    "fallback": "--mbx-container-text"
  },
  "--mbx-container-text-dark": {
    "description": "Container text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-text-dark",
    "fallback": "--mbx-container-text"
  }
}
